{
  "name": "yeti-finance-subgraph",
  "license": "MIT",
  "scripts": {
    "format": "npx prettier --write .",
    "codegen": "graph codegen",
    "build": "graph build",
    "deploy": "node ../../deployment/deployment.js --version=${npm_config_version} --type=${npm_config_type} --access=${npm_config_access} --fork=${npm_config_fork} --service=${npm_config_service} --protocol=${npm_config_protocol} --network=${npm_config_network} --target=${npm_config_target} --printlogs=${npm_config_printlogs} --merge=${npm_config_merge}",
    "create-local": "graph create --node http://localhost:8020/ yeti-finance",
    "remove-local": "graph remove --node http://localhost:8020/ yeti-finance",
    "deploy-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 yeti-finance"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "0.29.0",
    "@graphprotocol/graph-ts": "0.26.0"
  },
  "devDependencies": {
    "prettier": "^2.6.2"
  }
}
