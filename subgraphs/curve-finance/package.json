{
  "name": "curve",
  "license": "UNLICENSED",
  "scripts": {
    "codegen": "rm -rf generated && graph codegen",
    "build": "graph build",
    "prepare:yaml": "mustache protocols/curve-finance/networks/mainnet/mainnet.json protocols/curve-finance/templates/curve.template.yaml > subgraph.yaml",
    "deploy:subgraph": "graph deploy --product hosted-service ${npm_config_location}",
    "deploy": "node ../../deployment/deployment.js --version=${npm_config_version} --type=${npm_config_type} --access=${npm_config_access} --fork=${npm_config_fork} --service=${npm_config_service} --protocol=${npm_config_protocol} --network=${npm_config_network} --target=${npm_config_target} --printlogs=${npm_config_printlogs} --merge=${npm_config_merge}"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "^0.32.0",
    "@graphprotocol/graph-ts": "^0.27.0",
    "mustache": "^4.1.0"
  }
}
